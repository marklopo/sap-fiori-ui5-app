<mvc:View controllerName="app.customers.newproject.controller.Dash"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:core="sap.ui.core" xmlns:f="sap.f"
    xmlns:card="sap.f.cards" xmlns="sap.suite.ui.microchart" xmlns:l="sap.ui.layout"
    xmlns:micro="sap.ui.comp.smartmicrochart"
    xmlns:m="sap.m">
    <m:Page id="Z" title="Dashboard - {i18n>title}">
        <m:content>

            <f:Card id="_IDGenCard" class="sapUiMediumMargin" width="300px">
                <f:header>
                    <card:Header id="_IDGenHeader"
                        title="Info about Customers"
                        subtitle="Selected Customer data "
                        iconSrc="sap-icon://customer-view"
                        press="goToCustomersTable" />

                </f:header>
                <f:content>
                    <m:VBox id="_IDGenm:VBox"
                        height="110px"
                        class="sapUiSmallMargin"
                        justifyContent="SpaceBetween">
                        <m:ObjectStatus id="_IDGenObjectStatus"
                            text="Status: Ok" state="Success" />
                        <m:HBox id="_IDGenm:HBox2" renderType="Bare" justifyContent="SpaceBetween">
                            <m:Text id="_IDGenText3"
                                text=""
                                wrapping="true" />
                        </m:HBox>
                        <m:OverflowToolbar id="_IDGenOverflowToolbar2" design="Solid"
                            class="sapContrast">
                            <m:ToolbarSpacer id="_IDGenToolbarSpacer" />
                            <m:Button id="_IDGenm:Button"
                                text="More"
                                press="goToCustomersTable"
                                type="Transparent"
                                class="sapUiTinyMarginBegin" />
                        </m:OverflowToolbar>
                    </m:VBox>
                </f:content>
            </f:Card>
            <f:Card id="_IDGenCard2" class="sapUiMediumMargin" width="300px">
                <f:header>
                    <card:Header id="_IDGenHeader2"
                        title="Info about Products"
                        subtitle="All Products data "
                        iconSrc="sap-icon://product" />
                </f:header>
                <f:content>
                    <m:VBox id="_IDGenm:VBox2"
                        height="110px"
                        class="sapUiSmallMargin"
                        justifyContent="SpaceBetween">
                        <m:ObjectStatus id="_IDGenObjectStatus2"
                            text="Status: Ok" state="Success" />
                        <m:HBox id="_IDGenHBox" renderType="Bare" justifyContent="SpaceBetween">
                            <m:Text id="_IDGenText5"
                                text=""
                                wrapping="true" />
                        </m:HBox>
                        <m:HBox id="_IDGenm:HBox3" renderType="Bare" justifyContent="SpaceBetween">
                            <m:Text id="_IDGenText41"
                                text=""
                                wrapping="true" />

                            <m:OverflowToolbar id="_IDGenOverflowToolbar1" design="Solid"
                                class="sapContrast">
                                <m:ToolbarSpacer id="_IDGenToolbarSpacer2" />
                                <m:Button id="_IDGenm:Button3"
                                    text="More"
                                    press="goToProductsTable"
                                    type="Emphasized"
                                    class="sapUiTinyMarginBegin" />
                            </m:OverflowToolbar>

                        </m:HBox>
                    </m:VBox>
                </f:content>
            </f:Card>
            <f:Card id="_IDGenCard22" class="sapUiMediumMargin" width="300px">
                <f:header>
                    <card:Header id="_IDGenHeader22"
                        title="Info about Orders"
                        subtitle="Selected Orders data "
                        iconSrc="sap-icon://sales-order-item" />
                </f:header>
                <f:content>
                    <m:VBox id="_IDGenm:VBox22"
                        height="110px"
                        class="sapUiSmallMargin"
                        justifyContent="SpaceBetween">
                        <m:HBox id="_IDGenm:HBox32" renderType="Bare" justifyContent="SpaceBetween">
                            <m:ToolbarSpacer id="_IDGenToolbarSpacer1" />
                            <m:Button id="_IDGenm:Button32"
                                text="More"
                                press="goToOrdersTable"
                                type="Emphasized"
                                class="sapUiTinyMarginBegin" />
                        </m:HBox>

                    </m:VBox>

                </f:content>

            </f:Card>


        </m:content>

    </m:Page>

</mvc:View>